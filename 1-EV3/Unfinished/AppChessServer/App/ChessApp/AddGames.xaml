        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChessApp"
        mc:Ignorable="d"
        Title="AddGames" Height="302" Width="492"
        Closing="SecondaryWindow_Closing">
    <Grid Margin="-181,0,-81,0">
        <TextBox x:Name="textEmail1" HorizontalAlignment="Left" Margin="385,129,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160"/>
        <TextBox x:Name="textEmail2" HorizontalAlignment="Left" Margin="385,160,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="160"/>
        <Label Content="Player1 Email " HorizontalAlignment="Left" Margin="278,129,0,0" VerticalAlignment="Top"/>
        <Label Content="Player2 Email" HorizontalAlignment="Left" Margin="278,160,0,0" VerticalAlignment="Top" Width="99"/>
        <Button x:Name="buttonAdd" Content="Save&#xD;&#xA;" HorizontalAlignment="Left" Margin="278,61,0,0" VerticalAlignment="Top" Width="105" Click="addButton_Click"/>
        <Button x:Name="buttonCancel" Content="Cancel&#xD;&#xA;" HorizontalAlignment="Left" Margin="447,61,0,0" VerticalAlignment="Top" Width="105" Click="cancelButton_Click"/>
    </Grid>
</Window>
